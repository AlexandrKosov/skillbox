{
  "compilerOptions":{
     "target":"es5",
     "module":"commonjs",
     "lib":[
        "dom",
        "esnext"
     ],
     "allowJs":true,
     "jsx":"react",
     "sourceMap":true,
     "strict":true,
     "typeRoots":[
        "types"
     ],
     "allowSyntheticDefaultImports":true,
     "esModuleInterop":true,
     "resolveJsonModule":true,
     "skipLibCheck":true,
     "forceConsistentCasingInFileNames":true
  }
}
